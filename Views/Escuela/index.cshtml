@model Escuela;

@{
    ViewData["Title"] = "Información Escuela";
}

<section>
    <h1>Bienvenido a la Escuela: </h1>

    <h2>Informaciones de la Escuela</h2>
    <ul>
        <li>Nombre: @Model.Nombre </li>
        <li>Dirección: @Model.Dirección</li>
        <li>Año de fundación: @Model.AñoDeCreación</li>
    </ul>
</section>